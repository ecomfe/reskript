{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "build-vite",
          "analyze",
          "babel",
          "fix"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/config"
      ]
    },
    "build-vite": {
      "dependsOn": [
        "^build-vite"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build",
        "^lint"
      ]
    },
    "analyze": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
