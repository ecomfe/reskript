{
  "private": true,
  "name": "@reskript/showcase-todo",
  "version": "5.1.0",
  "description": "待办列表示例",
  "scripts": {
    "build": "rm -rf node_modules/.cache && skr build --clean --strict --build-target=stable",
    "build-vite": "rm -rf node_modules/.cache && skr build --clean --strict --build-target=stable --config=reskript-vite.config.ts",
    "analyze": "skr build --clean --strict --analyze --build-target=stable",
    "start": "skr dev",
    "start-vite": "skr dev --config=reskript-vite.config.ts",
    "lint": "skr lint --strict",
    "test": "npm run babel > /dev/null && npm run fix && npm run build && skr test --target=react && playwright test",
    "play": "skr play --config=reskript-e2e.config.ts --port=9875 src/components/Create/index.tsx",
    "play-vite": "skr play --config=reskript-vite-e2e.config.ts --port=9875 src/components/Create/index.tsx",
    "babel": "skr babel src/api/todo.ts",
    "fix": "echo '' >> src/components/App/index.less && skr lint --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@reskript/cli": "5.1.0",
    "@reskript/cli-babel": "5.1.0",
    "@reskript/cli-build": "5.1.0",
    "@reskript/cli-dev": "5.1.0",
    "@reskript/cli-lint": "5.1.0",
    "@reskript/cli-play": "5.1.0",
    "@reskript/cli-test": "5.1.0",
    "@reskript/config-lint": "5.1.0",
    "@reskript/plugin-inject-html": "5.1.0",
    "@reskript/plugin-qiankun": "5.1.0",
    "@reskript/settings": "5.1.0",
    "@testing-library/react": "^12.1.3",
    "@types/ramda": "^0.28.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/styled-components": "^5.1.24",
    "eslint": "^8.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stylelint": "^14.5.3",
    "typescript": "next",
    "vite": "^2.8.6",
    "webpack": "^5.69.1"
  },
  "dependencies": {
    "antd": "^4.18.9",
    "core-js": "^3.21.1",
    "localforage": "^1.10.0",
    "nanoid": "^3.3.1",
    "ramda": "^0.28.0",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.0.23"
  }
}
